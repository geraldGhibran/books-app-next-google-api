{
  "name": "frontend-assessment-test",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "clean": "rm -rf .next out",
    "test:update": "jest -u",
    "test:coverage": "jest --coverage",
    "watch:test": "jest --watch",
    "watch:ts": "tsc --noEmit -p . -w",
    "watch:lint": "esw --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx -w",
    "watch": "concurrently \"yarn watch:test\" \"yarn watch:ts\" \"yarn watch:lint\"",
    "check:lint": "eslint --ignore-path .gitignore . --ext .js,.jsx,.ts,.tsx",
    "check:prettier": "prettier --config .prettierrc ./src/ --check",
    "check:ts": "tsc",
    "checker": "yarn check:lint && yarn check:prettier && yarn tsc && jest",
    "format:lint": "eslint --fix ./src/ --ext .js,.jsx,.ts,.tsx",
    "format:prettier": "prettier --config .prettierrc ./src/ --write",
    "formatter": "yarn format:lint && yarn format:prettier"
  },
  "lint-staged": {
    "./src/**/*.{ts,js,jsx,tsx}": [
      "yarn formatter"
    ]
  },
  "dependencies": {
    "@headlessui/react": "^1.7.17",
    "@heroicons/react": "^2.1.1",
    "@types/react": "18",
    "@types/react-dom": "18",
    "axios": "^1.6.2",
    "next": "^13.4.7",
    "next-auth": "^4.24.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hot-toast": "^2.4.1",
    "styled-components": "^5.3.3"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@swc/plugin-styled-components": "^1.5.108",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/react-hooks": "^8.0.1",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^29.5.11",
    "@types/node": "17.0.21",
    "@types/node-fetch": "^2.6.10",
    "@types/styled-components": "^5.1.24",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.2",
    "axios-mock-adapter": "^1.22.0",
    "babel-plugin-jsx-remove-data-test-id": "^3.0.0",
    "concurrently": "^7.0.0",
    "cross-fetch": "^4.0.0",
    "eslint": "8.10.0",
    "eslint-config-next": "12.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-better-styled-components": "^1.1.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-watch": "^8.0.0",
    "jest": "^29.7.0",
    "jest-dom": "^4.0.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-styled-components": "^7.0.8",
    "lint-staged": "^12.3.5",
    "postcss": "^8.4.8",
    "prettier": "^2.5.1",
    "tailwindcss": "^3.0.23",
    "ts-jest": "^29.1.1",
    "typescript": "4.6.2",
    "whatwg-fetch": "^3.6.20"
  },
  "resolutions": {
    "@typescript-eslint/typescript-estree": "5.14.0",
    "@types/react": "17.0.14",
    "@types/react-dom": "17.0.14"
  }
}
